        "# Define the model - deep neural net, i.e., the number of input features and hidden nodes for each layer.\n",
        "\n",
        "import tensorflow as tf\n",
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import Dense\n",
        "model = Sequential()\n",
        "\n",
        "input_features = X_train_scaled.shape[1]\n",
        "\n",
        "\n",
        "nn = tf.keras.models.Sequential()\n",
        "hidden_nodes_layer1 = 30\n",
        "hidden_nodes_layer2 = 50\n",
        "hidden_nodes_layer3 = 20\n",
        "hidden_nodes_layer4 = 5\n",
        "# hidden_nodes_layer5 = 2\n",
        "\n",
        "\n",
        "# First hidden layer\n",
        "nn.add(Dense(hidden_nodes_layer1, activation='relu', input_dim=input_features))\n",
        "\n",
        "# Second hidden layer\n",
        "nn.add(Dense(hidden_nodes_layer2, activation='relu'))\n",
        "\n",
        "nn.add(Dense(hidden_nodes_layer3, activation='relu'))\n",
        "nn.add(Dense(hidden_nodes_layer4, activation='relu'))\n",
        "# nn.add(Dense(hidden_nodes_layer5, activation='relu'))\n",
        "\n",
        "\n",
        "# Output layer\n",
        "nn.add(Dense(1, activation='sigmoid'))\n",
        "\n",
        "\n",
        "nn.build(input_shape=(None, input_features))\n",
        "\n",
        "\n",
        "# Check the structure of the model\n",
